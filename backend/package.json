{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"DEBUG=nodemon:* ./bin/www\""
  },
  "scriptsComments": {
    "start": "runs prod server",
    "dev old command": "DEBUG=nodemon:* ./bin/www",
    "start old new command": "DEBUG=node:* ./bin/www",
    "start old command": "node ./bin/www"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "6.6.5",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.5.5"
  }
}
